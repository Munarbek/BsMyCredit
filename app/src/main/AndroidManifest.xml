<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.bssoft.bsmycredit">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.BSMyCredit">
        <activity android:name=".HelpActivity"></activity>
        <activity android:name=".NotificationActivity" />
        <activity android:name=".HistoryPaymentActivity" />
        <activity android:name=".BidListActivity" />
        <activity android:name=".BidActivity" />
        <activity android:name=".SingUpActivity" />
        <activity android:name=".GraphActivity" />
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:theme="@style/Theme.BSMyCreditActivity" />
    </application>

</manifest>